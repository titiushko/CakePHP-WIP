<IfModule mod_rewrite.c>
	Options +FollowSymLinks
	RewriteEngine On
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteRule ^(.*)$ /practicas/codeigniter/accesos_directos_a_html/index.php/$1 [L] 
</IfModule>
<IfModule !mod_rewrite.c>
	ErrorDocument 404 /practicas/codeigniter/accesos_directos_a_html/index.php 
</IfModule>